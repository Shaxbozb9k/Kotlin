fun main() {      
 // var ozgaruvchi
	// val ozgarmas
    
    
    
    
    //sanoqlarga oid --->2 types
    // Integer types Misol ---> 344642876 --> butun son
//    byte --> 8 bit
//    short --> 16 bit
//    int --> 32 bit
//    long --> 64 bit

// u ---> musbat sonlar ishlatganda son oldiga qoyiladi


	// Floating-point types misol---> 233.23423 --->  qoldiqli sonlar 
// float --> kichkina qoldiqlilar uchun  32-bit
// double --> Kotta proyectlar uchun ishlatiladi 64-bit
    
    
    println("My \tname is Shaxbozbek")
	println("My \bname is Shaxbozbek")
    println("My\nname is Shaxbozbek")
    println("My name  \\is\\ Shaxbozbek")
    println("My \"name\"  is Shaxbozbek")
    println("My 'name'  is Shaxbozbek")
    
    
    val rawString ="""
    	This is raw string.
    	This is raw string. (\n, \t)
        This is raw string.
    """.trimIndent()
println(rawString)    
    
    
    println("\uD83D\uDE0A")
    
    
    
    val a = 5
    val b = 6
    println(a == b)
    
    var Change_Name = "Shaxbozbek"
    Change_Name = "Shaxnoza"
    println(Change_Name)

}

fun calculateArea(width: Int, height: Int) = width * height


fun main() {   

	val roomArea = calculateArea(10,5)
	println("Room area: $roomArea")
}
